{
  "name": "lastjs",
  "version": "0.1.0",
  "description": "A minimal Next.js alternative with App Router and SSR",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "lastjs": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "default": "./dist/client/index.js"
    },
    "./router": {
      "types": "./dist/router/index.d.ts",
      "default": "./dist/router/index.js"
    },
    "./render": {
      "types": "./dist/render/index.d.ts",
      "default": "./dist/render/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "default": "./dist/vite/index.js"
    },
    "./navigation": {
      "types": "./dist/navigation/index.d.ts",
      "default": "./dist/navigation/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.5.2",
    "commander": "^14.0.0",
    "h3": "^1.15.3",
    "pathe": "^2.0.3",
    "picocolors": "^1.1.1",
    "vite": "^7.3.0"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "tsdown": "^0.19.0-beta.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "react",
    "ssr",
    "framework",
    "next",
    "app-router",
    "vite",
    "lastjs",
    "streaming"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/last.js.git",
    "directory": "packages/lastjs"
  }
}
