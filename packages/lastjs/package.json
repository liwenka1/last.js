{
  "name": "lastjs",
  "version": "0.1.0",
  "description": "A minimal Next.js alternative with App Router and SSR",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "lastjs": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./router": {
      "types": "./dist/router.d.mts",
      "default": "./dist/router.mjs"
    },
    "./render": {
      "types": "./dist/render.d.mts",
      "default": "./dist/render.mjs"
    },
    "./nitro": {
      "types": "./dist/nitro.d.mts",
      "default": "./dist/nitro.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.mts",
      "default": "./dist/vite.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@lastjs/cli": "workspace:*",
    "@lastjs/core": "workspace:*",
    "@lastjs/nitro": "workspace:*",
    "@lastjs/vite": "workspace:*"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "tsdown": "^0.19.0-beta.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "react",
    "ssr",
    "framework",
    "next",
    "app-router",
    "nitro",
    "vite",
    "lastjs"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/last.js.git",
    "directory": "packages/lastjs"
  }
}
