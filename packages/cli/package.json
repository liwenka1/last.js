{
  "name": "@lastjs/cli",
  "version": "0.1.0",
  "description": "CLI for Last.js framework",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "lastjs": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@lastjs/core": "workspace:*",
    "@lastjs/nitro": "workspace:*",
    "@lastjs/vite": "workspace:*",
    "@vitejs/plugin-react": "^5.1.2",
    "commander": "^14.0.2",
    "pathe": "^2.0.3",
    "picocolors": "^1.1.1",
    "vite": "^7.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsdown": "^0.19.0-beta.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "lastjs",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/last.js.git",
    "directory": "packages/cli"
  }
}
